android:layout_height="wrap_content"
android:typeface="normal"
android:textSize="12dp"
android:textColor="@color/lgdesc"
android:text="start or join encrypted P2P channels" /></LinearLayout></LinearLayout></LinearLayout></RelativeLayout>
			
			
			<RelativeLayout
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:paddingLeft="12dp"
